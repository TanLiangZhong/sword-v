    "file-saver": "^2.0.2",
    "fuse.js": "^3.4.6",
    "js-base64": "^2.5.2",
    "js-cookie": "^2.2.1",
    "js-md5": "^0.7.3",
    "jsonlint": "1.6.3",
    "jszip": "^3.2.2",
    "normalize.css": "^8.0.1",
